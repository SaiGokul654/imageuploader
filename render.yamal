services:
  - type: web
    name: imageuploader      # you can change the name if you like
    env: python
    buildCommand: "pip install -r requirements.txt"
    startCommand: "gunicorn imageuploader.wsgi:application"
    envVars:
      - key: DJANGO_SETTINGS_MODULE
        value: imageuploader.settings
      - key: PYTHON_VERSION
        value: 3.11
